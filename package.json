{
  "name": "squiz-platform",
  "version": "1.0.0",
  "description": "Comprehensive Quiz and Assessment Platform",
  "keywords": [
    "quiz",
    "education",
    "assessment",
    "react",
    "fastapi",
    "mongodb",
    "typescript"
  ],
  "homepage": "https://github.com/your-username/squiz-platform#readme",
  "bugs": {
    "url": "https://github.com/your-username/squiz-platform/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/squiz-platform.git"
  },
  "license": "MIT",
  "author": "Squiz Team",
  "main": "app/page.tsx",
  "scripts": {
    "backend:dev": "cd backend && source venv/bin/activate && python server.py",
    "build": "next build",
    "clean": "rm -rf node_modules frontend/node_modules backend/venv .next frontend/build",
    "dev": "next dev",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:up": "docker-compose up -d",
    "frontend:dev": "cd frontend && npm start",
    "lint": "next lint",
    "setup": "chmod +x setup.sh && ./setup.sh",
    "start": "next start",
    "test": "npm run test:frontend && npm run test:backend",
    "test:backend": "cd backend && python -m pytest",
    "test:frontend": "cd frontend && npm test"
  },
